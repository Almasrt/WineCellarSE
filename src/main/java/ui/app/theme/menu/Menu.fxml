<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import java.net.URL?>
<AnchorPane fx:id="menu" prefHeight="70.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.app.theme.menu.Menu">
    <stylesheets>
        <URL value="@/ui/app/theme/menu/Menu.css" />
    </stylesheets>
    <children>
        <GridPane fx:id="itemHolder" AnchorPane.leftAnchor="0.0" />
    </children>
    <properties>
        <!-- store controller at key "controller" in properties map -->
        <controller><fx:reference source="controller"/></controller>
    </properties>
</AnchorPane>
