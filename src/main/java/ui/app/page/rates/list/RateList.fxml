<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ui.app.page.rates.list.RateList"
            prefHeight="600.0" prefWidth="800.0">

    <children>

        <ScrollPane prefHeight="400.0" prefWidth="650.0" >

            <VBox style="-fx-content-display: center" prefWidth="650.0" alignment="CENTER">
                <Label text="Avis" alignment="CENTER" style="-fx-font-weight: bold; -fx-padding: 5; -fx-font-size: 20px;" />
                <GridPane fx:id="cardHolder" prefWidth="630.0" alignment="TOP_CENTER" layoutY="109.0" />
                <HBox fx:id="hbox"/>
            </VBox>

        </ScrollPane>
    </children>
    <properties>
        <!-- store controller at key "controller" in properties map -->
        <controller><fx:reference source="controller"/></controller>
    </properties>
</AnchorPane>
