    <stylesheets>
        <URL value="@/ui/app/page/company/referencing/ReferencingPage.css" />
    </stylesheets>
    <children>
        <Button mnemonicParsing="false" text="Sample" />
        <fx:include fx:id="referencingCreation" source="creation/ReferencingCreation.fxml" />
